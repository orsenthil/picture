{
  "manifest_version": 3,
  "name": "Picture of the Day",
  "version": "0.0.7",
  "description": "Transform your new tab daily pictures from NASA, Wikipedia, and Bing with AI enhanced information.",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:8000/*",
    "https://picture.learntosolveit.com/*",
    "https://api.nasa.gov/*",
    "https://www.googletagmanager.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' https://www.googletagmanager.com; object-src 'self'"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "picture-of-the-day@learntosolveit.com",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": [
          "storage",
          "https://picture.learntosolveit.com/*"
        ],
        "optional": [
          "https://api.nasa.gov/*",
          "https://www.googletagmanager.com/*"
        ]
      }
    }
  },
  "author": "Senthil Kumaran",
  "homepage_url": "https://picture.learntosolveit.com"
}
